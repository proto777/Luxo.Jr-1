<?xml version="1.0" ?>
<launch>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find lookso_jr_gazebo)/config/position_controller.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
  output="screen" ns="/lookso_jr" args="joint_state_controller
                                        joint1_position
                                        joint2_position
                                        joint3_position
                                        joint4_position"/>
</launch>
